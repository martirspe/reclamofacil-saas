{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "apps",
  "projects": {
    "landing": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/landing",
      "sourceRoot": "apps/landing/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/landing",
            "index": "apps/landing/src/index.html",
            "browser": "apps/landing/src/main.ts",
            "polyfills": [],
            "tsConfig": "apps/landing/tsconfig.app.json",
            "assets": [
              "apps/landing/src/favicon.ico",
              "apps/landing/src/assets"
            ],
            "styles": [
              "apps/landing/src/styles.css"
            ],
            "scripts": [],
            "server": "apps/landing/src/main.server.ts",
            "prerender": false,
            "ssr": {
              "entry": "apps/landing/src/main.server.ts"
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "1mb"
                }
              ],
              "outputHashing": "all"
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "landing:build:production"
            },
            "development": {
              "buildTarget": "landing:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4200
          }
        }
      }
    },
    "public-form": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/public-form",
      "sourceRoot": "apps/public-form/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/public-form",
            "index": "apps/public-form/src/index.html",
            "browser": "apps/public-form/src/main.ts",
            "polyfills": [],
            "tsConfig": "apps/public-form/tsconfig.app.json",
            "assets": [
              "apps/public-form/src/favicon.ico",
              "apps/public-form/src/assets"
            ],
            "styles": [
              "apps/public-form/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "500kb",
                  "maximumError": "2mb"
                }
              ],
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "apps/public-form/src/environments/environment.ts",
                  "with": "apps/public-form/src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "public-form:build:production"
            },
            "development": {
              "buildTarget": "public-form:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4201,
            "allowedHosts": [
              "localhost",
              ".reclamofacil.local"
            ]
          }
        }
      }
    },
    "dashboard": {
      "projectType": "application",
      "schematics": {},
      "root": "apps/dashboard",
      "sourceRoot": "apps/dashboard/src",
      "prefix": "app",
      "architect": {
        "build": {
          "builder": "@angular/build:application",
          "options": {
            "outputPath": "dist/dashboard",
            "index": "apps/dashboard/src/index.html",
            "browser": "apps/dashboard/src/main.ts",
            "polyfills": [
              "zone.js"
            ],
            "tsConfig": "apps/dashboard/tsconfig.app.json",
            "assets": [
              "apps/dashboard/src/favicon.ico",
              "apps/dashboard/src/assets"
            ],
            "styles": [
              "apps/dashboard/src/styles.css"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "1mb",
                  "maximumError": "3mb"
                }
              ],
              "outputHashing": "all",
              "fileReplacements": [
                {
                  "replace": "apps/dashboard/src/environments/environment.ts",
                  "with": "apps/dashboard/src/environments/environment.prod.ts"
                }
              ]
            },
            "development": {
              "optimization": false,
              "extractLicenses": false,
              "sourceMap": true
            }
          },
          "defaultConfiguration": "production"
        },
        "serve": {
          "builder": "@angular/build:dev-server",
          "configurations": {
            "production": {
              "buildTarget": "dashboard:build:production"
            },
            "development": {
              "buildTarget": "dashboard:build:development"
            }
          },
          "defaultConfiguration": "development",
          "options": {
            "port": 4202
          }
        }
      }
    }
  },
  "cli": {
    "analytics": false
  }
}